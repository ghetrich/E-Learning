<!-- Breadcomb area Start-->
<div class="breadcomb-area">
	<div class="container">
		<div class="row">
			<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
				<div class="row" style="margin-bottom: 40px; margin-top: 30px">
					<div class="col-lg-6 col-md-6 col-sm-6 col-xs-3"></div>
					<div class="col-lg-6 col-md-6 col-sm-6 col-xs-3">
						<div class="breadcomb-report-new">
							<button
								class="btn btn-icon-notika"
								id="toClients"
							>
								<i class="notika-icon notika-left-arrow"></i> Go Back
							</button>
						</div>
					</div>
				</div>

				<div class="breadcomb-list">
					<div class="row">
						<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
							<div class="breadcomb-wp">
								<div class="breadcomb-icon">
									<i class="notika-icon notika-windows"></i>
								</div>

								<div class="breadcomb-ctn">
									<h2><%= user.surname %> <%= user.othernames %></h2>
									<p>
										<%= user.username %>
										<!-- <span class="bread-ntd">Our Communities</span> -->
									</p>
								</div>
							</div>
						</div>
						<div class="col-lg-6 col-md-6 col-sm-6 col-xs-3">
							<div class="breadcomb-report">
								<button
									data-toggle="tooltip"
									data-placement="left"
									title="Download Report"
									class="btn"
								>
									<i class="notika-icon notika-sent"></i>
								</button>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<!-- Breadcomb area End-->

